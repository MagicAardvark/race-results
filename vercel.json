{
    "framework": "nextjs",
    "regions": ["iad1"],
    "buildCommand": "pnpm build:vercel",
    "functions": {
        "app/api/**": {
            "maxDuration": 30
        }
    },
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "SAMEORIGIN"
                }
            ]
        }
    ],
    "redirects": []
}
